{
  "name": "argus",
  "version": "0.1.0",
  "license": "Argus 2018",
  "default_language": "en",
  "languages": "hu",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "test": "ng test",
    "lint": "npx eslint . --ext .ts,.html -f html -o eslint.html",
    "e2e": "ng e2e",
    "preinstall": "npx npm-force-resolutions",
    "postinstall": "ngcc",
    "extract-i18n": "ng extract-i18n --output-path src/locales --format xliff && ng run argus:xliffmerge",
    "compress": "for lang in $npm_package_default_language $npm_package_languages; do gzip dist-production/$lang/*.js || true && gzip dist-production/$lang/*.css || true; done",
    "serve": "docker rm -fv arpi || true && docker run -d --name arpi -p 4200:8080 -v $(pwd)/nginx.conf:/etc/nginx/nginx.conf:ro -v $(pwd)/$DIST:/usr/share/nginx/html:ro nginx",
    "postbuild": "mv $DIST/en/* $DIST; rm -r $DIST/en"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^11.0.5",
    "@angular/cdk": "^11.0.3",
    "@angular/common": "^11.0.5",
    "@angular/compiler": "^11.0.5",
    "@angular/compiler-cli": "^11.0.5",
    "@angular/core": "^11.0.5",
    "@angular/flex-layout": "^11.0.0-beta.33",
    "@angular/forms": "^11.0.5",
    "@angular/localize": "^11.0.5",
    "@angular/material": "^11.0.3",
    "@angular/platform-browser": "^11.0.5",
    "@angular/platform-browser-dynamic": "^11.0.5",
    "@angular/platform-server": "^11.0.5",
    "@angular/router": "^11.0.5",
    "@danielmoncada/angular-datetime-picker": "^10.1.0",
    "@ngx-i18nsupport/tooling": "^8.0.3",
    "@types/jwt-decode": "^2.2.1",
    "humanize-duration-ts": "^2.1.1",
    "jwt-decode": "^2.2.0",
    "material-design-icons": "^3.0.1",
    "moment": "^2.29.1",
    "ngx-countdown": "^8.0.4",
    "ngx-i18nsupport": "^0.17.1",
    "rxjs": "^6.6.3",
    "socket.io-client": "^3.0.4",
    "tslib": "^2.0.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.1100.5",
    "@angular-eslint/builder": "0.8.0-beta.1",
    "@angular-eslint/eslint-plugin": "0.8.0-beta.1",
    "@angular-eslint/eslint-plugin-template": "0.8.0-beta.1",
    "@angular-eslint/schematics": "^0.8.0-beta.7",
    "@angular-eslint/template-parser": "0.8.0-beta.1",
    "@angular/cli": "^11.2.0",
    "@angular/language-service": "^11.0.5",
    "@ngx-i18nsupport/ngx-i18nsupport": "^1.1.6",
    "@types/humanize-duration": "^3.18.1",
    "@types/jasmine": "~3.6.0",
    "@types/jasminewd2": "^2.0.8",
    "@types/node": "^12.19.11",
    "@typescript-eslint/eslint-plugin": "4.3.0",
    "@typescript-eslint/parser": "4.3.0",
    "codelyzer": "^6.0.0",
    "eslint": "^7.16.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jsdoc": "30.7.6",
    "eslint-plugin-prefer-arrow": "1.2.2",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~5.0.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-cli": "~2.0.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "minimist": "^1.2.5",
    "protractor": "~7.0.0",
    "ts-node": "~8.5.4",
    "typescript": "^4.0.5"
  },
  "resolutions": {
    "serialize-javascript": "^2.1.1",
    "minimist": "^1.2.5"
  }
}
