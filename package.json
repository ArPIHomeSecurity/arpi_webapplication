{
  "name": "argus",
  "version": "0.1.0",
  "license": "Argus 2018",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "test": "ng test",
    "lint": "npx eslint . --ext .ts,.html -f html -o eslint.html",
    "e2e": "ng e2e",
    "postinstall": "ngcc",
    "extract-i18n": "ng extract-i18n --output-path src/locales --format xliff",
    "create-package": "tar -cvzf arpi-webapplication.tar.gz -C dist-production .",
    "serve-dev": "docker rm -fv arpi-webserver || true && docker run -d --name arpi-webserver -p 4200:8080 -v $(pwd)/arpi_dev_dhparams.pem:/etc/nginx/arpi_dev_dhparams.pem:ro -v $(pwd)/arpi_dev.crt:/etc/nginx/arpi_dev.crt:ro -v $(pwd)/arpi_dev.key:/etc/nginx/arpi_dev.key:ro -v $(pwd)/nginx.conf:/etc/nginx/nginx.conf:ro -v $(pwd)/dist-development:/usr/share/nginx/html:ro nginx",
    "serve-demo-dev": "docker rm -fv arpi-webserver || true && docker run -d --name arpi-webserver -p 4200:8080 -v $(pwd)/nginx.conf:/etc/nginx/nginx.conf:ro -v $(pwd)/dist-demo-dev:/usr/share/nginx/html:ro nginx",
    "serve-demo": "docker rm -fv arpi-webserver || true && docker run -d --name arpi-webserver -p 4200:8080 -v $(pwd)/nginx.conf:/etc/nginx/nginx.conf:ro -v $(pwd)/dist-demo:/usr/share/nginx/html:ro nginx"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^17.3.2",
    "@angular/cdk": "^17.3.2",
    "@angular/common": "^17.3.2",
    "@angular/compiler": "^17.3.2",
    "@angular/compiler-cli": "^17.3.2",
    "@angular/core": "^17.3.2",
    "@angular/forms": "^17.3.2",
    "@angular/localize": "^17.3.2",
    "@angular/material": "^17.3.2",
    "@angular/platform-browser": "^17.3.2",
    "@angular/platform-browser-dynamic": "^17.3.2",
    "@angular/platform-server": "^17.3.2",
    "@angular/router": "^17.3.2",
    "@capacitor/android": "^6.1.2",
    "@capacitor/angular": "^2.0.3",
    "@capacitor/app": "6.0.1",
    "@capacitor/core": "6.1.2",
    "@capacitor/haptics": "6.0.1",
    "@capacitor/keyboard": "6.0.2",
    "@capacitor/preferences": "^6.0.2",
    "@capacitor/status-bar": "6.0.1",
    "@danielmoncada/angular-datetime-picker": "^17.0.0",
    "@types/jwt-decode": "^2.2.1",
    "@uiowa/digit-only": "^3.2.1",
    "engine.io-parser": "5.2.1",
    "humanize-duration-ts": "^2.1.1",
    "jwt-decode": "^2.2.0",
    "material-design-icons": "^3.0.1",
    "moment": "^2.29.1",
    "ngx-countdown": "^16.0.0",
    "rxjs": "^7.8.1",
    "socket.io-client": "^4.7.5",
    "tslib": "^2.0.0",
    "zone.js": "^0.14.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^17.3.2",
    "@angular-eslint/builder": "^17.3.0",
    "@angular-eslint/eslint-plugin": "^17.3.0",
    "@angular-eslint/eslint-plugin-template": "^17.3.0",
    "@angular-eslint/template-parser": "^17.3.0",
    "@angular/cli": "^17.3.2",
    "@angular/language-service": "^17.3.2",
    "@capacitor/assets": "^3.0.5",
    "@capacitor/cli": "6.1.2",
    "@types/humanize-duration": "^3.18.1",
    "@types/jasmine": "~3.6.0",
    "@types/jasminewd2": "^2.0.8",
    "@types/node": "^12.19.11",
    "@typescript-eslint/eslint-plugin": "5.43.0",
    "@typescript-eslint/parser": "5.43.0",
    "codelyzer": "^6.0.0",
    "eslint": "^8.27.0",
    "eslint-plugin-import": "2.25.2",
    "eslint-plugin-jsdoc": "30.7.6",
    "eslint-plugin-prefer-arrow": "1.2.2",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.3.4",
    "karma-chrome-launcher": "~3.1.0",
    "karma-cli": "~2.0.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "minimist": "^1.2.5",
    "ng-extract-i18n-merge": "^2.11.0",
    "ts-node": "~8.5.4",
    "typescript": "^5.4.3"
  }
