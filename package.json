{
  "name": "argus",
  "version": "0.1.0",
  "license": "Argus 2018",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "test": "ng test",
    "lint": "npx eslint . --ext .ts,.html -f html -o eslint.html",
    "e2e": "ng e2e",
    "postinstall": "ngcc",
    "extract-i18n": "ng extract-i18n --output-path src/locales --format xliff",
    "create-package": "tar -cvzf arpi-webapplication.tar.gz -C dist-production .",
    "serve-dev": "docker rm -fv arpi || true && docker run -d --name arpi -p 4200:8080 -v $(pwd)/nginx.conf:/etc/nginx/nginx.conf:ro -v $(pwd)/dist-development/browser:/usr/share/nginx/html:ro nginx",
    "serve-demo-dev": "docker rm -fv arpi || true && docker run -d --name arpi -p 4200:8080 -v $(pwd)/nginx.conf:/etc/nginx/nginx.conf:ro -v $(pwd)/dist-demo-dev/browser:/usr/share/nginx/html:ro nginx",
    "serve-demo": "docker rm -fv arpi || true && docker run -d --name arpi -p 4200:8080 -v $(pwd)/nginx.conf:/etc/nginx/nginx.conf:ro -v $(pwd)/dist-demo/browser:/usr/share/nginx/html:ro nginx"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^18.2.8",
    "@angular/cdk": "^18.2.8",
    "@angular/common": "^18.2.8",
    "@angular/compiler": "^18.2.8",
    "@angular/compiler-cli": "^18.2.8",
    "@angular/core": "^18.2.8",
    "@angular/forms": "^18.2.8",
    "@angular/localize": "^18.2.8",
    "@angular/material": "^18.2.8",
    "@angular/platform-browser": "^18.2.8",
    "@angular/platform-browser-dynamic": "^18.2.8",
    "@angular/platform-server": "^18.2.8",
    "@angular/router": "^18.2.8",
    "@danielmoncada/angular-datetime-picker": "^18.1.0",
    "@uiowa/digit-only": "^3.2.1",
    "humanize-duration-ts": "^2.1.1",
    "jwt-decode": "^4.0.0",
    "material-design-icons": "^3.0.1",
    "moment": "^2.29.1",
    "ngx-countdown": "^18.0.0",
    "rxjs": "^7.8.1",
    "socket.io-client": "^4.8.0",
    "tslib": "^2.7.0",
    "zone.js": "^0.14.10"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^18.2.8",
    "@angular-eslint/builder": "^18.3.1",
    "@angular-eslint/eslint-plugin": "^18.3.1",
