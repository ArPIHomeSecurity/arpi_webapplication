{
  "name": "argus",
  "version": "0.1.0",
  "license": "Argus 2018",
  "default_language": "en",
  "languages": "hu",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "translate-hu": "ng xi18n --output-path src/locales --locale en && xliffmerge --profile xliffmerge.json en hu",
    "build-development": "ng build --output-path dist-development",
    "build-default-language": "ng build --aot prod --env=prod --output-path dist-production-$npm_package_default_language --sourcemap=false",
    "build-translations": "for lang in $npm_package_languages; do ng build --aot -prod --env=prod --output-path=dist-production-$lang --bh /$lang/ --i18n-file=src/locales/messages.$lang.xlf --i18n-format=xlf --locale=$lang --sourcemap=false; done",
    "build-compress": "for lang in $npm_package_default_language $npm_package_languages; do gzip dist-production-$lang/*.js || true && gzip dist-production-$lang/*.css || true; done",
    "build-production": "npm run build-default-language && npm run build-translations && npm run build-compress"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^6.1.7",
    "@angular/cdk": "^7.0.0-beta.0",
    "@angular/cli": "^6.2.1",
    "@angular/common": "^6.1.7",
    "@angular/compiler": "^6.1.7",
    "@angular/compiler-cli": "^6.1.7",
    "@angular/core": "^6.1.7",
    "@angular/flex-layout": "^6.0.0-beta.18",
    "@angular/forms": "^6.1.7",
    "@angular/http": "^6.1.7",
    "@angular/material": "^7.0.0-beta.0",
    "@angular/platform-browser": "^6.1.7",
    "@angular/platform-browser-dynamic": "^6.1.7",
    "@angular/platform-server": "^6.1.7",
    "@angular/router": "^6.1.7",
    "@types/jwt-decode": "^2.2.1",
    "core-js": "^2.4.1",
    "jwt-decode": "^2.2.0",
    "material-design-icons": "^3.0.1",
    "rxjs": "^6.3.2",
    "rxjs-compat": "^6.0.0-rc.0",
    "socket.io-client": "^2.1.1",
    "zone.js": "^0.8.18"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.8.0",
    "@angular/language-service": "^6.1.7",
    "@types/jasmine": "~2.5.53",
    "@types/jasminewd2": "~2.0.2",
    "@types/node": "~6.0.60",
    "codelyzer": "^4.4.4",
    "jasmine-core": "~2.6.2",
    "jasmine-spec-reporter": "~4.1.0",
    "karma": "~1.7.0",
    "karma-chrome-launcher": "~2.1.1",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^1.2.1",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "~5.1.2",
    "ts-node": "~3.2.0",
    "tslint": "~5.3.2",
    "typescript": "^2.9.2",
    "webpack": "^4.17.2"
  }
}
