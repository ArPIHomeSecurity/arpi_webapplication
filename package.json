{
  "name": "argus",
  "version": "0.1.0",
  "license": "Argus 2018",
  "default_language": "en",
  "languages": "hu",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "translate-hu": "ng xi18n --output-path locales --i18n-locale en && xliffmerge --profile xliffmerge.json en hu",
    "build-default-language": "ng build --configuration=production-$npm_package_default_language",
    "build-translations": "ng build --configuration=production-hu",
    "build-compress": "for lang in $npm_package_default_language $npm_package_languages; do gzip dist-production-$lang/*.js || true && gzip dist-production-$lang/*.css || true; done"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^8.2.14",
    "@angular/cdk": "^8.2.3",
    "@angular/common": "^8.2.14",
    "@angular/compiler": "^8.2.14",
    "@angular/compiler-cli": "^8.2.14",
    "@angular/core": "^8.2.14",
    "@angular/flex-layout": "^8.0.0-beta.27",
    "@angular/forms": "^8.2.14",
    "@angular/material": "^8.2.3",
    "@angular/platform-browser": "^8.2.14",
    "@angular/platform-browser-dynamic": "^8.2.14",
    "@angular/platform-server": "^8.2.14",
    "@angular/router": "^8.2.14",
    "@types/jwt-decode": "^2.2.1",
    "jwt-decode": "^2.2.0",
    "material-design-icons": "^3.0.1",
    "ng-pick-datetime": "^7.0.0",
    "ng2-timezone-selector": "^0.2.4",
    "ngx-i18nsupport": "^0.17.1",
    "rxjs": "^6.5.4",
    "rxjs-tslint": "^0.1.7",
    "socket.io-client": "^2.3.0",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.803.24",
    "@angular/cli": "^8.3.24",
    "@angular/language-service": "^8.2.14",
    "@types/jasmine": "^3.5.2",
    "@types/jasminewd2": "^2.0.8",
    "@types/node": "^12.12.26",
    "@types/socket.io-client": "^1.4.32",
    "codelyzer": "^5.2.1",
    "core-js": "^2.5.7",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "^4.4.1",
    "karma-chrome-launcher": "~3.1.0",
    "karma-cli": "~2.0.0",
    "karma-coverage-istanbul-reporter": "^2.1.1",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.5.2",
    "protractor": "^5.4.3",
    "ts-node": "~8.5.4",
    "tslint": "^5.20.1",
    "typescript": "^3.5.3",
    "webpack": "^4.41.5",
    "webpack-dev-server": "^3.10.2"
  },
  "resolutions": {
    "serialize-javascript": "^2.1.1"
  }
}
