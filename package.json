{
  "name": "argus",
  "version": "0.1.0",
  "license": "Argus 2018",
  "default_language": "en",
  "languages": "hu",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "postinstall": "ngcc",
    "extract-i18n": "ng xi18n --output-path src/locales --format xliff && ng run argus:xliffmerge",
    "compress": "for lang in $npm_package_default_language $npm_package_languages; do gzip dist-production/$lang/*.js || true && gzip dist-production/$lang/*.css || true; done",
    "serve": "docker rm -fv arpi || true && docker run -d --name arpi -p 4200:8080 -v $(pwd)/nginx.conf:/etc/nginx/nginx.conf:ro -v $(pwd)/$DIST:/usr/share/nginx/html:ro nginx",
    "postbuild": "mv $DIST/en/* $DIST; rm -r $DIST/en"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^9.1.12",
    "@angular/cdk": "^9.2.4",
    "@angular/common": "^9.1.12",
    "@angular/compiler": "^9.1.12",
    "@angular/compiler-cli": "^9.1.12",
    "@angular/core": "^9.1.12",
    "@angular/flex-layout": "^9.0.0-beta.31",
    "@angular/forms": "^9.1.12",
    "@angular/localize": "~9.0.7",
    "@angular/material": "^9.2.4",
    "@angular/platform-browser": "^9.1.12",
    "@angular/platform-browser-dynamic": "^9.1.12",
    "@angular/platform-server": "^9.1.12",
    "@angular/router": "^9.1.12",
    "@ngx-i18nsupport/tooling": "^8.0.3",
    "@types/jwt-decode": "^2.2.1",
    "humanize-duration": "^3.24.0",
    "jwt-decode": "^2.2.0",
    "material-design-icons": "^3.0.1",
    "moment-timezone": "^0.5.32",
    "ng-pick-datetime": "^7.0.0",
    "ngx-countdown": "^8.0.4",
    "ngx-i18nsupport": "^0.17.1",
    "rxjs": "^6.6.3",
    "rxjs-tslint": "^0.1.8",
    "socket.io-client": "^2.3.1",
    "tslib": "^1.14.1",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.900.7",
    "@angular/cli": "^9.1.12",
    "@angular/language-service": "^9.1.12",
    "@ngx-i18nsupport/ngx-i18nsupport": "^1.1.6",
    "@types/humanize-duration": "^3.18.1",
    "@types/jasmine": "^3.6.1",
    "@types/jasminewd2": "^2.0.8",
    "@types/node": "^12.19.4",
    "@types/socket.io-client": "^1.4.34",
    "codelyzer": "^5.2.2",
    "core-js": "^2.6.11",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "^4.4.1",
    "karma-chrome-launcher": "~3.1.0",
    "karma-cli": "~2.0.0",
    "karma-coverage-istanbul-reporter": "^2.1.1",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.5.4",
    "protractor": "^5.4.4",
    "ts-node": "~8.5.4",
    "tslint": "^5.20.1",
    "typescript": "^3.9.7",
    "webpack": "^4.44.2",
    "webpack-dev-server": "^3.11.0"
  },
  "resolutions": {
    "serialize-javascript": "^2.1.1"
  }
}
